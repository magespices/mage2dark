{
  "name": "magespices/mage2dark",
  "type": "magento2-theme",
  "license": "OSL-3.0",
  "authors": [
    {
      "name": "Przemys≈Çaw Jankowski",
      "email": "przemek@qsolutionsstudio.com"
    },
    {
      "name": "Jakub Winkler",
      "email": "jwinkler@qsolutionsstudio.com",
    }
  ],
  "description": "Magento 2 Admin Dark Theme",
  "homepage": "https://mage2dark.net/",
  "keywords": [
    "magento",
    "magento2",
    "admin theme",
    "core",
    "dark mode"
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "require": {
    "magento/framework": "100.*",
    "magento/module-backend": "100.*"
  },
  "autoload": {
    "files": [
      "src/module-magespices-core/registration.php",
      "src/module-magespices-mage2dark/registration.php",
      "src/theme-adminhtml-mage2dark/registration.php"
    ],
    "psr-4": {
      "MageSpices\\Core\\": "src/module-magespices-core",
      "MageSpices\\Mage2Dark": "src/module-magespices-mage2dark"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}